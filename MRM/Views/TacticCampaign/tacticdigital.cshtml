

<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <h4 class="modal-title">
                <span>Digital Touchpoint</span>
            </h4>
        </div>
        <div class="modal-body">
            <section class="content">
                <div class="box box-default">
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Tactic Type</label>
                                    <div class="form-group">
                                        <input id="txtType" type="text" value="" class="form-control" placeholder="Tactic Type" disabled="disabled" />
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Tactic Name</label>
                                    <div class="form-group">
                                        <input id="txtTacticName" type="text" value="" class="form-control" placeholder="Tactic Name" disabled="disabled" />
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Tactic ID</label>
                                    <div class="form-group">
                                        <input id="txtTacticID" type="text" value="" class="form-control" placeholder="Tactic ID" disabled="disabled" />
                                    </div>
                                </div>
                            </div>
                            <!-- /.col -->
                        </div>

                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Source</label><span class="text-danger Industrymanage-mandate">*</span>

                                    <div class="form-group">
                                        <input type="text" id="txtSource" value="" class="form-control" placeholder="Source" />
                                        <div id="dvsource"></div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Content</label><span class="text-danger Industrymanage-mandate">*</span>
                                    <div class="form-group">
                                        <input type="text" id="txtContent" value="" class="form-control" placeholder="Content" />
                                        <div id="dvContent"></div>
                                    </div>
                                </div>
                            </div>
                            <!-- /.col -->
                        </div>

                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Medium</label><span class="text-danger Industrymanage-mandate">*</span>
                                    <div class="form-group">
                                        <input type="text" id="txtMedium" value="" class="form-control" placeholder="Medium" />
                                        <div id="dvMedium"></div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Terms</label><span class="text-danger Industrymanage-mandate">*</span>
                                    <div class="form-group">
                                        <input type="text" id="txtTerms" value="" class="form-control" placeholder="Terms" />
                                        <div id="dvTerms"></div>
                                    </div>
                                </div>
                            </div>
                            <!-- /.col -->
                        </div>

                        <div class="row">
                            <div class="col-md-10"></div>
                            <div class="col-md-2">
                                <input type="button" class="btn btn-primary" value="Add" onclick="AddRow();" />
                                <input type="hidden" id="hdnIndex"/>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12 form-group">
                                <div class="table-responsive">
                                    @*<table id="gridReport" class="display compact table table-striped table-bordered table-hover dataTable no-footer" width="100%"></table>*@
                                    <table id="gridReport" class="table table-bordered table-hover">
                                            <thead>
                                                <tr>     
                                                    <td>Digital ID</td>                                               
                                                    <td>Source</td>
                                                    <td>Medium</td>
                                                    <td>Content</td>
                                                    <td>Term</td>
                                                    <td>Action</td>
                                                </tr>
                                            </thead>
                                            <tbody>  
                                                                                            
                                            </tbody>
                                        </table>
                                </div>
                            </div>
                            <!-- /.col -->
                        </div>
                        <!-- /.row -->


                    </div>

                    <!-- /.box-body -->
                </div>
            </section>
        </div>
        <div class="modal-footer">
            <div>
                <input type="button" onclick="return fDeleteDigitalpoint();" class="btn btn-danger" value="Delete" />
                @*<input type="button" class="btn btn-primary" value="Save Draft" />*@
                <input type="button" onclick="return savedigitalpoint();" class="btn btn-info" value="Submit & Export" />
                <button id="btnclose" type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>

